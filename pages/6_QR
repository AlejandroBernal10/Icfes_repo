import streamlit as st
st.set_page_config(page_title="QR", page_icon=":🏿:", layout="wide")


st.title("QRs")


st.subheader("Pagina web")
col1, col2, col3 = st.columns([1, 2, 6])

# Dejando espacio vacío en las columnas externas
with col1:
    st.write("")

with col3:
    st.write("")

# Mostrando la imagen en la columna central
with col2:
    st.image("qr_pw.png", width=900)


st.subheader("Dashboard")
col1, col2, col3 = st.columns([1, 2, 6])

# Dejando espacio vacío en las columnas externas
with col1:
    st.write("")

with col3:
    st.write("")

# Mostrando la imagen en la columna central
with col2:
    st.image("qr_ds.png", width=900)
